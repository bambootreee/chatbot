{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/ask",
      "dest": "/api/ask.js"
    },
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    }
  ]
}
